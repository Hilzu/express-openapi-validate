{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "no-console": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": [true, "ajv/lib/refs"],
    "object-literal-sort-keys": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
